textures/metal_misc/metal_m02ss
{
    qer_editorimage textures/metal_misc/metal_m02ss
	diffusemap textures/metal_misc/metal_m02ss
	bumpmap textures/metal_misc/metal_m02ss_n
	specularmap textures/metal_misc/metal_m02ss_s
	q3map_lightmapsamplesize 1
	surfaceparm metalsteps
	{
		map textures/effects/tinfx
		rgbGen identity
		tcGen environment
	}
	{
		map textures/metal_misc/metal_m02ss
		blendFunc GL_ONE GL_ONE_MINUS_SRC_ALPHA
		rgbGen identity
	}
	{
		map $lightmap
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
	}
}

