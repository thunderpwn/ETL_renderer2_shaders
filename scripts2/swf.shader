textures/swf/fuse_box_ad
{
    qer_editorimage textures/swf/fuse_box_ad
	diffusemap textures/swf/fuse_box_ad
	specularmap textures/swf/fuse_box_ad_s
	bumpmap textures/swf/fuse_box_ad_n
	polygonOffset
	surfaceparm pointlight
	surfaceparm trans
	{
		map textures/swf/fuse_box_ad
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbGen vertex
		tcmod rotate 120
	}
}

